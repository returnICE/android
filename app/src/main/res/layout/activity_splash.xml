<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.github.jorgecastillo.FillableLoader
        android:id="@+id/fillableLoader"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:fl_clippingTransform="waves"
        app:fl_fillColor="#f87979"
        app:fl_fillDuration="2000"
        app:fl_originalHeight="1562"
        app:fl_originalWidth="1162"
        app:fl_strokeColor="#f87979"
        app:fl_strokeDrawingDuration="1000"
        app:fl_strokeWidth="2dp" />

</androidx.constraintlayout.widget.ConstraintLayout>